
public class NodeDriverTest {
	Node firstNode;
	Node lastNode;
	public void addNode(Node node) {
		if(firstNode == null) {
			firstNode = node;
			lastNode = node;
		}else {
			lastNode.nextNode = node;
			lastNode = node;
		}
	}
	public static void main(String args[]) {
		Node node = new Node();
		//node.setTag("1", "Durant");
		//node.setTag("2", "KD");
		
		System.out.println(node.map);
	}
		
}
